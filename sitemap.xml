---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <!-- 首页 -->
    <url>
        <loc>https://jayjd.github.io</loc>
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        <changefreq>daily</changefreq>
        <priority>1.0</priority>
    </url>

    <!-- TV页面 -->
    <url>
        <loc>https://jayjd.github.io/tv</loc>
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        <changefreq>daily</changefreq>
        <priority>0.9</priority>
    </url>

    <!-- 车载页面 -->
    <url>
        <loc>https://jayjd.github.io/car</loc>
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        <changefreq>daily</changefreq>
        <priority>0.8</priority>
    </url>

    <!-- 更新页面 -->
    <url>
        <loc>https://jayjd.github.io/update</loc>
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        <changefreq>daily</changefreq>
        <priority>0.7</priority>
    </url>

    <!-- 消息页面 -->
    <url>
        <loc>https://jayjd.github.io/message</loc>
        <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        <changefreq>weekly</changefreq>
        <priority>0.6</priority>
    </url>

    <!-- 动态生成的页面 -->
    {% for page in site.pages %}
        {% if page.layout != null and page.layout != 'feed' %}
        <url>
            <loc>{{ site.url }}{{ page.url }}</loc>
            <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
            <changefreq>weekly</changefreq>
            <priority>0.5</priority>
        </url>
        {% endif %}
    {% endfor %}

    <!-- 文章页面 -->
    {% for post in site.posts %}
    <url>
        <loc>{{ site.url }}{{ post.url }}</loc>
        <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
        <changefreq>monthly</changefreq>
        <priority>0.4</priority>
    </url>
    {% endfor %}
</urlset>